<?xml version="1.0"?>
<include width="580" height="444">
	<!--login page-->
	<caption pos="0,0,-0,-7" name="cap_login_main">
		<caption pos="0,0,-0,30" name="cap_login_head">
			<!--title bar-->
			<icon pos="10,8" src="ICON_LOGO:16" />
			<text pos="[5,4">@string/title</text>
			<text pos="[0,4">V</text>
			<text pos="[0,4">@string/ver</text>
			<imgbtn name="btn_close" skin="_skin.sys.btn.close" pos="-40,0" tip="close" animate="1" />
			<imgbtn name="btn_min" skin="_skin.sys.btn.minimize" pos="-75,0" animate="1" />
			<imgbtn name="btn_setting" skin="btn_login_setting" pos="-105,0" animate="1" />
		</caption>
		<caption pos="0,30,-0,-0">
			<!--body-->
			<caption pos="0,0,-0,-0">
				<img pos="207,61,309,147" skin="img_head_default" name="img_avatar" />
				<comboview name="cbx_account" pos="162,156,372,184" dropDown="0" dropHeight="200" animateTime="0" class="cls_edit" curSel="0" id_text="3">
					<liststyle class="cls_edit" colorBkgnd="#ffffff">
						<template itemHeight="44" colorHover="#cccccc" colorSelected="#0000ff">
							<img id="1" pos="1,|-20,@32,@32" skin="img_avatars" />
							<text id="2" pos="49,9">想象元素</text>
							<text id="3" pos="48,26" colorText="#ff0000" />
							<imgbtn id="4" pos="-24,|-8,-10,|7" skin="btn_list_close" tip="删除账号信息" />
						</template>
					</liststyle>
					<editstyle vcenter="1" colorText="#000000" align="left" colorBkgnd="#FFFFFF" cueText="输入帐号" />
				</comboview>
				<window pos="162,190,372,218" class="cls_edit" colorBkgnd="#ffffff">
					<edit pos="0,0,-19,-0" margin="0" password="1" mouseRelay="1" cueText="输入密码" />
					<imgbtn pos="-20,3" skin="btn_keyboard" mouseRelay="1" />
				</window>
				<button pos="200,224" size="104, 32" colorText="#282828ff" skin="btn_login" name="btn_login">登陆</button>
				<button pos="200,280" size="104, 32" colorText="#000000" skin="_skin.sys.btn.normal" name="btn_reg">注册</button>
			</caption>
		</caption>
	</caption>
</include>
